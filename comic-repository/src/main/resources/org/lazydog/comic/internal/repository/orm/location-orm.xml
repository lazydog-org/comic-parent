<?xml version="1.0" encoding="UTF-8"?>
<entity-mappings
    xmlns="http://java.sun.com/xml/ns/persistence/orm"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xsi:schemaLocation="http://java.sun.com/xml/ns/persistence/orm
                        http://java.sun.com/xml/ns/persistence/orm_2_0.xsd"
    version="2.0">
    <package>org.lazydog.comic.model</package>
    <entity class="Location" access="FIELD" metadata-complete="true">
        <table name="location" schema="comic_collection"/>
        <named-query name="Location.findAll">
            <query>select l from Location l</query>
        </named-query>
        <attributes>
            <basic name="name">
                <column name="name" nullable="false"/>
            </basic>
        </attributes>
    </entity>
</entity-mappings>
