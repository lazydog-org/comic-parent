<?xml version="1.0" encoding="UTF-8"?>
<entity-mappings
    xmlns="http://java.sun.com/xml/ns/persistence/orm"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xsi:schemaLocation="http://java.sun.com/xml/ns/persistence/orm
                        http://java.sun.com/xml/ns/persistence/orm_2_0.xsd"
    version="2.0">
    <package>org.lazydog.comic.model</package>
    <entity class="Distribution" access="FIELD" metadata-complete="true">
        <table name="distribution" schema="comic_collection"/>
        <named-query name="Distribution.findAll">
            <query>select d from Distribution d</query>
        </named-query>
        <attributes>
            <basic name="value">
                <column name="value" nullable="false" unique="true"/>
            </basic>
        </attributes>
    </entity>
</entity-mappings>
