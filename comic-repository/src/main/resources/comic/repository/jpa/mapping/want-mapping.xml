<?xml version="1.0" encoding="UTF-8"?>
<entity-mappings
    xmlns="http://java.sun.com/xml/ns/persistence/orm"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xsi:schemaLocation="http://java.sun.com/xml/ns/persistence/orm 
                        http://java.sun.com/xml/ns/persistence/orm_1_0.xsd"
    version="1.0">

    <package>comic.api.model</package>
    <entity class="Want" access="FIELD" metadata-complete="true">
        <table name="want" schema="comic_collection"/>
        <named-query name="Want.findAll">
            <query>select w from Want w</query>
        </named-query>
        <attributes>
            <many-to-one name="comic" fetch="EAGER">
                <join-column name="comic_id" nullable="false"/>
            </many-to-one>
        </attributes>
    </entity>
</entity-mappings>
