<?xml version="1.0" encoding="UTF-8"?>
<faces-config 
    xmlns="http://java.sun.com/xml/ns/javaee"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xsi:schemaLocation="http://java.sun.com/xml/ns/javaee
                        http://java.sun.com/xml/ns/javaee/web-facesconfig_1_2.xsd"
    version="1.2">

    <component>
        <component-type>org.lazydog.comic.faces.MonthYearSelectItems</component-type>
        <component-class>org.lazydog.comic.faces.component.MonthYearSelectItems</component-class>
    </component>
    <component>
        <component-type>org.lazydog.comic.faces.TogglePanel</component-type>
        <component-class>org.lazydog.comic.faces.component.TogglePanel</component-class>
    </component>
    <component>
        <component-type>org.lazydog.comic.faces.TogglePanelTrigger</component-type>
        <component-class>org.lazydog.comic.faces.component.TogglePanelTrigger</component-class>
    </component>
    <render-kit>
        <renderer>
            <component-family>org.lazydog.comic.faces.TogglePanel</component-family>
            <renderer-type>org.lazydog.comic.faces.HtmlTogglePanelRenderer</renderer-type>
            <renderer-class>org.lazydog.comic.faces.renderer.html.HtmlTogglePanelRenderer</renderer-class>
        </renderer>
    </render-kit>
    <render-kit>
        <renderer>
            <component-family>org.lazydog.comic.faces.TogglePanelTrigger</component-family>
            <renderer-type>org.lazydog.comic.faces.HtmlTogglePanelTriggerRenderer</renderer-type>
            <renderer-class>org.lazydog.comic.faces.renderer.html.HtmlTogglePanelTriggerRenderer</renderer-class>
        </renderer>
    </render-kit>
</faces-config>
