<?xml version="1.0" encoding="UTF-8"?>
<faces-config 
    xmlns="http://java.sun.com/xml/ns/javaee"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xsi:schemaLocation="http://java.sun.com/xml/ns/javaee
                        http://java.sun.com/xml/ns/javaee/web-facesconfig_2_0.xsd"
    version="2.0">
    <managed-bean>
        <managed-bean-name>categoryBean</managed-bean-name>
        <managed-bean-class>org.lazydog.comic.manager.bean.CategoryBean</managed-bean-class>
        <managed-bean-scope>request</managed-bean-scope></managed-bean>
    <managed-bean>
        <managed-bean-name>comicBean</managed-bean-name>
        <managed-bean-class>org.lazydog.comic.manager.bean.ComicBean</managed-bean-class>
        <managed-bean-scope>request</managed-bean-scope></managed-bean>
    <managed-bean>
        <managed-bean-name>comicCharacterBean</managed-bean-name>
        <managed-bean-class>org.lazydog.comic.manager.bean.ComicCharacterBean</managed-bean-class>
        <managed-bean-scope>request</managed-bean-scope></managed-bean>
    <managed-bean>
        <managed-bean-name>comicFilterBean</managed-bean-name>
        <managed-bean-class>org.lazydog.comic.manager.bean.ComicFilterBean</managed-bean-class>
        <managed-bean-scope>request</managed-bean-scope>
        <managed-property>
            <property-name>comicBean</property-name>
            <value>#{comicBean}</value>
        </managed-property></managed-bean>
    <managed-bean>
        <managed-bean-name>comicGradeBean</managed-bean-name>
        <managed-bean-class>org.lazydog.comic.manager.bean.ComicGradeBean</managed-bean-class>
        <managed-bean-scope>request</managed-bean-scope></managed-bean>
    <managed-bean>
        <managed-bean-name>comicTypeBean</managed-bean-name>
        <managed-bean-class>org.lazydog.comic.manager.bean.ComicTypeBean</managed-bean-class>
        <managed-bean-scope>request</managed-bean-scope></managed-bean>
    <managed-bean>
        <managed-bean-name>comicUploadBean</managed-bean-name>
        <managed-bean-class>org.lazydog.comic.manager.bean.ComicUploadBean</managed-bean-class>
        <managed-bean-scope>request</managed-bean-scope></managed-bean>
    <managed-bean>
        <managed-bean-name>distributionBean</managed-bean-name>
        <managed-bean-class>org.lazydog.comic.manager.bean.DistributionBean</managed-bean-class>
        <managed-bean-scope>request</managed-bean-scope></managed-bean>
    <managed-bean>
        <managed-bean-name>haveBean</managed-bean-name>
        <managed-bean-class>org.lazydog.comic.manager.bean.HaveBean</managed-bean-class>
        <managed-bean-scope>request</managed-bean-scope></managed-bean>
    <managed-bean>
        <managed-bean-name>imageBean</managed-bean-name>
        <managed-bean-class>org.lazydog.comic.manager.bean.ImageBean</managed-bean-class>
        <managed-bean-scope>request</managed-bean-scope></managed-bean>
    <managed-bean>
        <managed-bean-name>imageFilterBean</managed-bean-name>
        <managed-bean-class>org.lazydog.comic.manager.bean.ImageFilterBean</managed-bean-class>
        <managed-bean-scope>request</managed-bean-scope>
        <managed-property>
            <property-name>imageBean</property-name>
            <value>#{imageBean}</value>
        </managed-property></managed-bean>
    <managed-bean>
        <managed-bean-name>imageTypeBean</managed-bean-name>
        <managed-bean-class>org.lazydog.comic.manager.bean.ImageTypeBean</managed-bean-class>
        <managed-bean-scope>request</managed-bean-scope></managed-bean>
    <managed-bean>
        <managed-bean-name>imprintBean</managed-bean-name>
        <managed-bean-class>org.lazydog.comic.manager.bean.Imprint</managed-bean-class>
        <managed-bean-scope>request</managed-bean-scope></managed-bean>
    <managed-bean>
        <managed-bean-name>locationBean</managed-bean-name>
        <managed-bean-class>org.lazydog.comic.manager.bean.LocationBean</managed-bean-class>
        <managed-bean-scope>request</managed-bean-scope></managed-bean>
    <managed-bean>
        <managed-bean-name>locationReportBean</managed-bean-name>
        <managed-bean-class>org.lazydog.comic.manager.report.bean.LocationReportBean</managed-bean-class>
        <managed-bean-scope>request</managed-bean-scope></managed-bean>
    <managed-bean>
        <managed-bean-name>navigator</managed-bean-name>
        <managed-bean-class>org.lazydog.comic.manager.helper.bean.Navigator</managed-bean-class>
        <managed-bean-scope>request</managed-bean-scope></managed-bean>
    <managed-bean>
        <managed-bean-name>professionBean</managed-bean-name>
        <managed-bean-class>org.lazydog.comic.manager.bean.ProfessionBean</managed-bean-class>
        <managed-bean-scope>request</managed-bean-scope></managed-bean>
    <managed-bean>
        <managed-bean-name>publisherBean</managed-bean-name>
        <managed-bean-class>org.lazydog.comic.manager.bean.PublisherBean</managed-bean-class>
        <managed-bean-scope>request</managed-bean-scope></managed-bean>
    <managed-bean>
        <managed-bean-name>titleBean</managed-bean-name>
        <managed-bean-class>org.lazydog.comic.manager.bean.TitleBean</managed-bean-class>
        <managed-bean-scope>request</managed-bean-scope></managed-bean>
    <managed-bean>
        <managed-bean-name>titleFilterBean</managed-bean-name>
        <managed-bean-class>org.lazydog.comic.manager.bean.TitleFilterBean</managed-bean-class>
        <managed-bean-scope>request</managed-bean-scope>
        <managed-property>
            <property-name>titleBean</property-name>
            <value>#{titleBean}</value>
        </managed-property></managed-bean>
    <managed-bean>
        <managed-bean-name>titleReportBean</managed-bean-name>
        <managed-bean-class>org.lazydog.comic.manager.report.bean.TitleReportBean</managed-bean-class>
        <managed-bean-scope>request</managed-bean-scope></managed-bean>
    <managed-bean>
        <managed-bean-name>titleTypeBean</managed-bean-name>
        <managed-bean-class>org.lazydog.comic.manager.bean.TitleTypeBean</managed-bean-class>
        <managed-bean-scope>request</managed-bean-scope></managed-bean>
    <managed-bean>
        <managed-bean-name>traitBean</managed-bean-name>
        <managed-bean-class>org.lazydog.comic.manager.bean.TraitBean</managed-bean-class>
        <managed-bean-scope>request</managed-bean-scope></managed-bean>
    <managed-bean>
        <managed-bean-name>userBean</managed-bean-name>
        <managed-bean-class>org.lazydog.comic.manager.bean.UserBean</managed-bean-class>
        <managed-bean-scope>request</managed-bean-scope></managed-bean>
</faces-config>









