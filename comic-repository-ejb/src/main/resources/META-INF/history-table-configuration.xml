<?xml version="1.0" encoding="UTF-8"?>
<configuration
    xmlns="http://lazydog.org/xml/ns/historytable"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xsi:schemaLocation="http://lazydog.org/xml/ns/historytable
                        http://lazydog.org/xml/ns/historytable/history-table-configuration.xsd">

    <table-data-source>jdbc/ComicCollectionPool</table-data-source>
    <history-table-data-source>jdbc/ComicHistoryPool</history-table-data-source>
    <mapping>
        <entity-class-name>org.lazydog.comic.model.Category</entity-class-name>
    </mapping>
    <mapping>
        <entity-class-name>org.lazydog.comic.model.Character</entity-class-name>
    </mapping>
    <mapping>
        <entity-class-name>org.lazydog.comic.model.Comic</entity-class-name>
    </mapping>
    <mapping>
        <entity-class-name>org.lazydog.comic.model.ComicGrade</entity-class-name>
    </mapping>
    <mapping>
        <entity-class-name>org.lazydog.comic.model.ComicType</entity-class-name>
    </mapping>
    <mapping>
        <entity-class-name>org.lazydog.comic.model.Creator</entity-class-name>
    </mapping>
    <mapping>
        <entity-class-name>org.lazydog.comic.model.Distribution</entity-class-name>
    </mapping>
    <mapping>
        <entity-class-name>org.lazydog.comic.model.Have</entity-class-name>
    </mapping>
    <mapping>
        <entity-class-name>org.lazydog.comic.model.Image</entity-class-name>
    </mapping>
    <mapping>
        <entity-class-name>org.lazydog.comic.model.ImageType</entity-class-name>
    </mapping>
    <mapping>
        <entity-class-name>org.lazydog.comic.model.Imprint</entity-class-name>
    </mapping>
    <mapping>
        <entity-class-name>org.lazydog.comic.model.Location</entity-class-name>
    </mapping>
    <mapping>
        <entity-class-name>org.lazydog.comic.model.Person</entity-class-name>
    </mapping>
    <mapping>
        <entity-class-name>org.lazydog.comic.model.Profession</entity-class-name>
    </mapping>
    <mapping>
        <entity-class-name>org.lazydog.comic.model.Publisher</entity-class-name>
    </mapping>
    <mapping>
        <entity-class-name>org.lazydog.comic.model.Title</entity-class-name>
    </mapping>
    <mapping>
        <entity-class-name>org.lazydog.comic.model.TitleType</entity-class-name>
    </mapping>
    <mapping>
        <entity-class-name>org.lazydog.comic.model.Trait</entity-class-name>
    </mapping>
    <mapping>
        <entity-class-name>org.lazydog.comic.model.User</entity-class-name>
    </mapping>
    <mapping>
        <entity-class-name>org.lazydog.comic.model.UserPreference</entity-class-name>
    </mapping>
    <mapping>
        <entity-class-name>org.lazydog.comic.model.Want</entity-class-name>
    </mapping>
</configuration>
